{
    "name": "sepehr/phpunit-selenium-webdriver",
    "description": "Webdriver-backed Selenium testcase for PHPUnit with fluent testing API.",
    "keywords": ["phpunit", "testing", "selenium", "laravel", "integration", "acceptance", "webdriver"],
    "license": "MIT",
    "authors": [
        {
            "name": "Sepehr Lajevardi",
            "email": "lajevardi@gmail.com",
            "homepage": "https://github.com/sepehr"
        }
    ],
    "support": {
        "source": "https://github.com/sepehr/phpunit-selenium-webdriver",
        "issues": "https://github.com/sepehr/phpunit-selenium-webdriver/issues"
    },
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0",
        "facebook/webdriver": "^1.1",
        "codacy/coverage": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Sepehr\\PHPUnitSelenium\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sepehr\\PHPUnitSelenium\\Tests\\Unit\\": "tests/unit/",
            "Sepehr\\PHPUnitSelenium\\Tests\\Functional\\": "tests/functional/"
        }
    }
}
